<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kr.swjch.source.moimmap.mapper.moimmapMapper">

	<select id="getMoimMap" resultType="moimmapVO">
		select
			id,
			region,
			area_code,
			name,
			postal_code,
			address,
			phone,
			manager_mobile,
			manager,
			latitude,
			longitude
		from MOIMMAP;
	</select>

</mapper>